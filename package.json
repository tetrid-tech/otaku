{
  "name": "my-eliza-app",
  "version": "1.0.0",
  "description": "Custom Eliza App with React UI",
  "type": "module",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "turbo run build && bun run start-server.ts",
    "dev:watch": "turbo run dev --parallel",
    "start": "bun run start-server.ts",
    "build": "bun run build:backend && bun run build:frontend",
    "build:all": "turbo run build",
    "build:backend": "bun run build.ts",
    "build:frontend": "vite build",
    "type-check": "tsc --noEmit"
  },
  "packageManager": "bun@1.2.21",
  "workspaces": [
    "src/plugins/*"
  ],
  "dependencies": {
    "@elizaos/api-client": "latest",
    "@elizaos/cli": "latest",
    "@elizaos/core": "latest",
    "@elizaos/plugin-bootstrap": "workspace:*",
    "@elizaos/plugin-anthropic": "^1.0.6",
    "@elizaos/plugin-openai": "^1.5.15",
    "@elizaos/plugin-sql": "latest",
    "@elizaos/server": "latest",
    "@tanstack/react-query": "^5.29.0",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "turbo": "^2.3.3",
    "typescript": "^5.6.3",
    "vite": "^6.0.1"
  }
}
